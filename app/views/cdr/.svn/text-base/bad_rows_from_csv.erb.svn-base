<table class="maintable" width="100%">
  <% if @rows and @rows.size > 0 %>
      <% @rows.each_with_index do |row, index| %>
        <tr class="row<%= (index.to_i % 2).to_s %>">
          <% row.each_with_index do |column, index2| %>
              <% if column[0].include?('col_') %>
                  <td id="row_<%= index %>_col_<%= index2 %>">
                    <%= column[1] %>
                  </td>
              <% end %>
          <% end %>
          <% if params[:action] == 'bad_rows_from_csv' %>
            <td id="row_<%= index %>_error"><%= nice_cdr_import_error(row['nice_error'], row['owner_id']) %></td>
          <% end %>
        </tr>
      <% end %>
  <% end %>
</table>