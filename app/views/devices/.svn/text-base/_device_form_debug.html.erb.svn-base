<% if device_type == "SIP" or !reseller? %>
<tr>
  <td colspan="2" height="20"></td>
</tr>
<tr>
  <td colspan="2" class="bottom_border">
    <b><%= _('Debug') %></b>
  </td>
</tr>
<tr>
  <td colspan="2" height="10"></td>
</tr>
<% end %>
<%
   if device
     device_process_sipchaninfo = device.process_sipchaninfo
   else
     device_process_sipchaninfo = Confline.get_value("Default_device_process_sipchaninfo", session[:user_id])
   end
%>
<% if device_type == "SIP" %>
    <tr>
      <td><%= _('Process_SIPCHANINFO') %>:</td>
      <td><%= check_box_tag 'process_sipchaninfo', "1", device_process_sipchaninfo.to_i == 1 %></td>
    </tr>
<% end %>