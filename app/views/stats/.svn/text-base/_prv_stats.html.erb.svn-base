<table width="100%" class="bordered">
  <tr>
    <td width="50%" align="center">
      <table class="maintable" width="70%">
        <tr>
          <th align="left"><%= _('ANSWERED') %></th>
          <td align="right"><%= answered %></td>
          <td align="right"><%= asr_calls %> %</td>
        </tr>
        <tr>
          <th align="left"><%= _('NO_ANSWER') %></th>
          <td align="right"><%= no_answer %></td>
          <td align="right"><%= no_answer_calls_pr %> %</td>
        </tr>
        <tr>
          <th align="left"><%= _('BUSY') %></th>
          <td align="right"><%= busy %></td>
          <td align="right"><%= busy_calls_pr %> %</td>
        </tr>
        <tr>
          <th align="left"><%= _('FAILED') %></th>
          <td align="right"><%= failed %></td>
          <td align="right"><%= failed_calls_pr %> %</td>
        </tr>
      </table>
    </td>
    <td align="center">
      <div id="flashcontent1" style="padding-top:20px;"><b>You need to upgrade your Flash Player</b></div>
      <br/>
    </td>
  </tr>
</table>

<br/>

<table width="100%" class="bordered">
  <tr>
    <td width="100%" valign="top">
      <table width=100%>
        <tr>
          <td align="center">
            <br/><br/>
            <div id="flashcontent5"><b>You need to upgrade your Flash Player</b></div>
          </td>
          <td align="center">
            <br/><br/>
            <div id="flashcontent6"><b>You need to upgrade your Flash Player</b></div>
          </td>
          <% if can_see_finances? || accountant? %>
              <td align="center">
                <br/><br/>
                <div id="flashcontent7"><b>You need to upgrade your Flash Player</b></div>
              </td>
          <% end %>
        </tr>
      </table>
    </td>
  </tr>
</table>

<script type="text/javascript">
    var flashcontent1 = new SWFObject("<%= Web_Dir %>/swf/ampie.swf", 'ampie', '550', '250', '9', '#FFFFFF');
    flashcontent1.addVariable('path', "<%= Web_Dir %>/swf/");
    flashcontent1.addVariable('settings_file', escape("<%= Web_Dir %>/swf/pie4.xml"));
    flashcontent1.addParam('wmode', 'transparent');
    flashcontent1.addVariable('chart_data', "<%= lv_Calls_graph %>");
    flashcontent1.addVariable('additional_chart_settings', escape("<settings><labels><label><x>0</x><y>20</y><rotate>false</rotate><align>center</align><text_size>15</text_size><text><![CDATA[<b></b>]]></text></label></labels></settings>"));
    flashcontent1.write('flashcontent1');

    var flashcontent5 = new SWFObject("<%= Web_Dir %>/swf/amline.swf", 'amline', '370', '277', '8', '#FFFFFF');
    flashcontent5.addVariable('path', "<%= Web_Dir %>/swf/");
    flashcontent5.addVariable('settings_file', escape("<%= Web_Dir %>/swf/amline8.xml"));
    flashcontent5.addParam('wmode', 'transparent');
    flashcontent5.addVariable('chart_data', "<%= lv_Avg_Calltime_graph %>");
    flashcontent5.addVariable('additional_chart_settings', encodeURI("<settings><labels><label><x>0</x><y>20</y><rotate>false</rotate><align>center</align><text_size>15</text_size><text><![CDATA[<b><%= "#{_('avg_call_time')} (s)" %></b>]]></text></label></settings>"));
    flashcontent5.write('flashcontent5');

    var flashcontent6 = new SWFObject("<%= Web_Dir %>/swf/amline.swf", 'amline', '370', '277', '8', '#FFFFFF');
    flashcontent6.addVariable('path', "<%= Web_Dir %>/swf/");
    flashcontent6.addVariable('settings_file', escape("<%= Web_Dir %>/swf/amline1.xml"));
    flashcontent6.addParam('wmode', 'transparent');
    flashcontent6.addVariable('chart_data', "<%= lv_Asr_graph %>");
    flashcontent6.addVariable('additional_chart_settings', encodeURI("<settings><labels><label><x>0</x><y>20</y><rotate>false</rotate><align>center</align><text_size>15</text_size><text><![CDATA[<b><%= "#{_('ASR')}: #{asr_calls} %" %></b>]]></text></label></settings>"));
    flashcontent6.write('flashcontent6');

    if (<%= can_see_finances? || accountant? %>) {
        var flashcontent7 = new SWFObject("<%= Web_Dir %>/swf/amline.swf", 'amline', '370', '277', '8', '#FFFFFF');
        flashcontent7.addVariable('path', "<%= Web_Dir %>/swf/");
        flashcontent7.addVariable('settings_file', escape("<%= Web_Dir %>/swf/amline3.xml"));
        flashcontent7.addParam('wmode', 'transparent');
        flashcontent7.addVariable('chart_data', "<%=  lv_Profit_graph %>");
        flashcontent7.addVariable('additional_chart_settings', encodeURI("<settings><labels><label><x>0</x><y>20</y><rotate>false</rotate><align>center</align><text_size>15</text_size><text><![CDATA[<b><%= _('Profit') %></b>]]></text></label></labels><chart><graphs><graph><title>aaaaaa</title></graph><graph><title>aaaaaa</title></graph></settings>"));
        flashcontent7.write('flashcontent7');
    }
</script>